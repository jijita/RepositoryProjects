@foreach (var user in Model)
{
    <tr>
        <td>
            @user
        </td>

        <td>
            <button id="deleteBtn" title=@GestionEcole.Resources.Views.Administration._UsersInRole.DeleteBtnTitle class="btn btn-default" data-user=@user><span class="glyphicon glyphicon-minus"></span></button>
        </td>
    </tr>
}